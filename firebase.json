{
    "storage": {
        "rules": "storage.rules"
    },
    "hosting": {
        "public": "./",
        "ignore": [
            "database-rules.json",
            "functions"
        ],
        "headers": [{
            "source": "**/*.@(js|html)",
            "headers": [{
                "key": "Cache-Control",
                "value": "max-age=0"
            }]
        }]
    }
}